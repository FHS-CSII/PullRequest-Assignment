package wang_jason;

import java.util.ArrayList;

/**@author Jason Wang
 * **/
public class Exercises {
	public int threeAndFive(){
		int sum = 0;
		for (int i = 0; i < 1000; i++){
			if (i % 3 == 0 || i % 5 == 0){
				sum = sum + i;
			}
		}
		
		return sum;
	}
	
	public int evenFibSum(){
		int a = 1, b = 1;
		
		int sum = 0;
		while (a <= 4000000){
			if (a % 2 == 0)
				 sum = sum + a;
			int temp = a;
			a = b;
			b = temp + b;
				
		}
		
		return sum;
				       
	}
	
	public int largestPrimeFactor(){
		
	}
	
	public static void main(String[] args){
		System.out.println(evenFibSum());
	}
}
