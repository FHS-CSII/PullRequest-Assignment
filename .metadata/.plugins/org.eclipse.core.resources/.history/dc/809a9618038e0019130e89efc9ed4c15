	public static int threeAndFive()
	{
		int three = 3;
		int five = 5;
		int sum = 0;
		while(three <= 1000)
		{
			three +=3;
		}
		while (five <= 1000)
		{
			five +=5;
		}
		sum = three + five;
		return sum;
	}
	
