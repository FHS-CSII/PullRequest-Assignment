package Bhatt_Sahil;
public class Exercise 
{
	public static int threeAndFive()
	{
		int h = 0;
		for(int i=0; i<1000; i++){
			if(i%3==0 || i%5==0)
				h+=i;
		}
		return h;
	}
	
	
	public int evenFibSum()
	{
		int sum =2;
		int num1=1;
		int num2=2;
		int num=0;
		
		while (num<4000000){
			num=num1+num2;
			num1=num2;
			num2=num;
			if(num%2==0)
			{
				sum=sum+num;
			}
		}
	}

	public int largestPrimeFactor()
	{
		int i;
		
	}
	//4613732
	//6857
}
